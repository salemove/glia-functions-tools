# Debug settings
DEBUG=false
LOG_LEVEL=info

# Forwarding settings
{{#if includeForwarding}}
FORWARDING_URL={{forwardingUrl}}
{{#if authType}}
{{#eq authType "api-key"}}
API_KEY=your-api-key
{{/eq}}
{{#eq authType "bearer"}}
API_KEY=your-bearer-token
{{/eq}}
{{#eq authType "basic"}}
API_USERNAME=your-username
API_PASSWORD=your-password
{{/eq}}
{{/if}}
{{/if}}

# Data handling
{{#if includeFiltering}}
FILTER_PII=true
{{/if}}